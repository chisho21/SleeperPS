    <command:details>
      <command:name>Get-SleeperLeague</command:name>
      <command:verb>Get</command:verb>
      <command:noun>SleeperLeague</command:noun>
      <maml:description>
        <maml:para>Retrieves detailed information for one or more Sleeper leagues by LeagueId.</maml:para>
